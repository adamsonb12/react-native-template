{
  "name": "react-native-dialed-in",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "codegen": "mkdir -p /types && npx apollo client:codegen --tagName=gql --outputFlat --endpoint=http://localhost:3000 --target=typescript --includes=\"**/*.tsx\" types/",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx,.json",
    "lint.fix": "eslint . --ext .ts,.tsx,.js,.jsx,.json --fix",
    "format": "prettier --check .",
    "format.fix": "prettier --write ."
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@expo/webpack-config": "^0.17.0",
    "@react-native-community/datetimepicker": "6.2.0",
    "@react-navigation/native": "^6.0.12",
    "@react-navigation/native-stack": "^6.8.0",
    "@types/graphql": "^14.5.0",
    "@types/react": "~18.0.0",
    "@types/react-native": "~0.69.1",
    "date-fns": "^2.29.3",
    "expo": "~46.0.9",
    "expo-linear-gradient": "~11.4.0",
    "expo-localization": "~13.1.0",
    "expo-secure-store": "~11.3.0",
    "expo-status-bar": "~1.4.0",
    "graphql": "^16.6.0",
    "i18n-js": "^4.1.1",
    "lottie-react-native": "5.1.3",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "react-native": "0.69.5",
    "react-native-dotenv": "^3.3.1",
    "react-native-gesture-handler": "2.5.0",
    "react-native-paper": "5.0.0-rc.5",
    "react-native-reanimated": "~2.9.1",
    "react-native-reanimated-carousel": "^3.0.4",
    "react-native-safe-area-context": "4.3.1",
    "react-native-screens": "~3.15.0",
    "react-native-web": "~0.18.7",
    "styled-components": "^5.3.5",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1"
